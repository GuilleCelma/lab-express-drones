<h2>Available drones</h2>

{{#each Drones}}
    <div>
        <h3>{{name}}</h3>
        <h3>{{propellers}} </h3>
        <h3>{{maxSpeed}}</h3>
        <a href="/drones/{{_id}}/edit">Edit this drone</a>
        <form action="/drones/{{_id}}/delete" method="POST">
            <input type="submit" value="Delete">
        </form>
        
    </div>

{{/each}}